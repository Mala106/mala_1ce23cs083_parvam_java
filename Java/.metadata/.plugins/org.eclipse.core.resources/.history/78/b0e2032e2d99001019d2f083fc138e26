package com.encapsulation;

class Student{
	private String name;
	private int id;
	private int roll;
	private int age;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		name = name;
	}
}

public class EncapsulationInJava {
	public static void main(String[] args) {
		Student s1 = new Student();
	}
}
